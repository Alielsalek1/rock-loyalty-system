{
  "AllowedHosts": "*",
  "JwtOptions": {
    "ExpirationInMinutes": 35,
    "RefreshTokenExpirationInDays": 90,
    "JwtCookieOptions": {
      "HttpOnly": false,
      "Secure": true,
      "SameSite": "strict"
    }
  },
    "IpRateLimiting": {
      "EnableEndpointRateLimiting": true,
      "StackBlockedRequests": false,
      "RealIpHeader": "X-Real-IP",
      "ClientIdHeader": "X-ClientId",
      "HttpStatusCode": 429,
      "GeneralRules": [
        {
          "Endpoint": "*",
          "Period": "1m",
          "Limit": 100
        }
      ]
    },
    "AllowedUserAgents": [
      "Mozilla/5.0",  
      "Chrome",       
      "Safari",       
      "Edge",         
      "Opera",        
      "Trident",      
      "MSIE"          
    ]
}